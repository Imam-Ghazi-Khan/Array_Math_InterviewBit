
    public ArrayList<Interval> mergeOverlapppingIntervals(ArrayList<Interval> intervals) {
        
        if(intervals.size()<=1)
        {
            return intervals;
        }
        
        Collections.sort(intervals, new Comparator<Interval>()
        {
            public int compare(Interval i1,Interval i2)
            {
                return Integer.compare(i1.start,i2.start);
            }
        });
        
        ArrayList<Interval> output=new ArrayList();
        
        Interval lastResult = intervals.get(0);
        
        for(int i=1;i<intervals.size();i++)
        {
            Interval currentInterval = intervals.get(i);
            
            if(currentInterval.start<=lastResult.end)
            {   
                lastResult.end=Math.max(lastResult.end,currentInterval.end);
            }
            else
            {   
                output.add(lastResult);
                lastResult = currentInterval;
            }
        }
        
        output.add(new Interval(lastResult.start,lastResult.end)) ;
        return output;
    }
